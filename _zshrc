export ZSH=$HOME/envconf/oh-my-zsh
export ZSH_THEME="codeland"
export DISABLE_AUTO_TITLE="true"
export DISABLE_AUTO_UPDATE="true"

plugins=(archlinux command-coloring compleat dirpersist extract git \
    history-substring-search git-flow github perl ruby)

source $ZSH/oh-my-zsh.sh

