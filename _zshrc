# path
export ZSH=$HOME/code/oh-my-zsh

# theme
export ZSH_THEME="prose"

# plugin
plugins=(command-coloring gem git github perl ruby)

# title
export DISABLE_AUTO_TITLE="true"

# update
export DISABLE_AUTO_UPDATE="true"

# source
source $ZSH/oh-my-zsh.sh
